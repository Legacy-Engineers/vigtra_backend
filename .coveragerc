[run]
source = .
omit = 
    */venv/*
    */env/*
    */migrations/*
    manage.py
    */settings/*
    */tests/*
    */test_*.py
    */__pycache__/*
    */node_modules/*
    */static/*
    */media/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = Vigtra Backend Coverage Report